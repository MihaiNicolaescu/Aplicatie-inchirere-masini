<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane id="container" prefHeight="720.0" prefWidth="1240.0" stylesheets="@data/style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Login">
   <children>
      <Pane id="loginBox" layoutX="923.0" layoutY="291.0" prefHeight="300.0" prefWidth="250.0" stylesheets="@data/style.css">
         <children>
            <PasswordField fx:id="password" layoutX="15.0" layoutY="174.0" prefHeight="35.0" prefWidth="220.0" promptText="Password">
               <font>
                  <Font size="18.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="DEFAULT" />
               </cursor></PasswordField>
            <Button fx:id="loginBtn" layoutX="14.0" layoutY="236.0" mnemonicParsing="false" onAction="#loginMethod" prefHeight="35.0" prefWidth="100.0" text="Login">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="registerBtn" layoutX="136.0" layoutY="236.0" mnemonicParsing="false" onAction="#registerMethod" prefHeight="35.0" prefWidth="100.0" text="Register">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <TextField fx:id="username" layoutX="15.0" layoutY="115.0" prefHeight="35.0" prefWidth="220.0" promptText="Username">
               <font>
                  <Font size="18.0" />
               </font></TextField>
            <Label layoutX="16.0" layoutY="73.0" text="Login">
               <font>
                  <Font size="23.0" />
               </font>
            </Label>
            <Label fx:id="messageError" layoutX="4.0" prefHeight="75.0" prefWidth="242.0" textFill="#e8e4e4" wrapText="true">
               <font>
                  <Font size="16.0" />
               </font>
            </Label>
         </children>
      </Pane>
      <Button fx:id="exitBtn" layoutX="1148.0" layoutY="635.0" mnemonicParsing="false" onAction="#exit" prefHeight="50.0" prefWidth="50.0" text="Exit" />
   </children>
</AnchorPane>
